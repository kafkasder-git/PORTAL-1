{
  "name": "dernek-nextjs",
  "version": "0.1.0",
  "description": "Dernek YÃ¶netim Sistemi - Next.js 16 + Appwrite. See README.md for SDK usage guide.",
  "private": true,
  "scripts": {
    "dev": "next dev --turbo",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:error-boundaries": "tsx scripts/test-error-boundaries.ts",
    "test:loading-states": "tsx scripts/test-loading-states.ts",
    "test:suspense": "tsx scripts/test-suspense-boundaries.ts",
    "test:all-boundaries": "npm run test:error-boundaries && npm run test:loading-states && npm run test:suspense",
    "test:boundaries:watch": "nodemon --watch src --exec npm run test:all-boundaries",
    "e2e": "playwright test",
    "e2e:ui": "playwright test --ui",
    "analyze": "ANALYZE=true next build",
    "debug:hydration": "tsx scripts/debug-hydration.ts",
    "test:prod": "bash scripts/test-production-build.sh",
    "clean": "rm -rf .next node_modules/.cache",
    "clean:all": "rm -rf .next node_modules package-lock.json && npm install",
    "setup:appwrite": "tsx scripts/setup-appwrite.ts",
    "validate:config": "tsx scripts/validate-config.ts",
    "test:connectivity": "tsx scripts/test-connectivity.ts",
    "test:mock-api": "tsx scripts/test-mock-api.ts",
    "diagnose": "tsx scripts/diagnose-appwrite.ts",
    "health:check": "curl http://localhost:3000/api/health?detailed=true",
    "test:full-system": "tsx scripts/full-system-test.ts",
    "test:browsers": "tsx scripts/test-browser-compatibility.ts",
    "test:prod-enhanced": "tsx scripts/test-production-build-enhanced.ts",
    "test:integration": "npm run test:full-system && npm run test:browsers",
    "test:all": "npm run test && npm run test:e2e && npm run test:all-boundaries && npm run test:integration"
  },
